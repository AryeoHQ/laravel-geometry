{
    "name": "spinen/laravel-geometry",
    "description": "Wrapper over the geoPHP Class to make it integrate with Laravel better.",
    "keywords": [
        "geometry",
        "geoPHP",
        "laravel",
        "library",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Thai-Son Le",
            "email": "thaison.le@spinen.com"
        },
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "~5.1",
        "phayes/geophp": "~1.2"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.1",
        "phpunit/phpunit": "~4.0",
        "psy/psysh": "~0.5.1",
        "satooshi/php-coveralls": "~0.6.1",
        "symfony/var-dumper": "2.7.*"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\Geometry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\Geometry\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
